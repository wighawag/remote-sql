{
  "name": "remote-sql-libsql",
  "version": "0.0.4",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "./dist",
    "./src"
  ],
  "dependencies": {
    "remote-sql": "workspace:*"
  },
  "devDependencies": {
    "@libsql/core": "^0.6.0",
    "pkgroll": "^2.0.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@libsql/core": "^0.6.0"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch"
  }
}